{% extends 'base.html.twig' %}

{% block title %}Formula 1 races in 2024
{% endblock %}

{% block body %}


    <h1>F1 races in 2024</h1>
    {% for data in data.MRData.RaceTable.Races %}

        <li>
            <img src="https://source.unsplash.com/300x300?{{race.raceName}}" class="card-img-top w-100" alt="...">
            <a href={{data.url}}>{{data.raceName|e}}</a>
            <p>Circuit:
                {{data.Circuit.circuitId}}</p>
            <p>Date:
                {{data.date}}</p>
            <p>Location:
                {{data.Circuit.Location.locality}}</p>
        </li>
    {% endfor %}


{% endblock %}